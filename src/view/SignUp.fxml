<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="ap" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.SignUp_Controller">
   <children>
      <VBox fx:id="titleBox" alignment="CENTER_LEFT" prefHeight="576.0" prefWidth="1050.0">
         <children>
            <Label fx:id="titleText" alignment="CENTER" prefHeight="150.0" prefWidth="1050.0" style="-fx-font-size: 50;" text="회원가입" />
            <HBox fx:id="signUpBox" prefHeight="480.0" prefWidth="1050.0">
               <children>
                  <VBox fx:id="textBox" alignment="TOP_CENTER" maxHeight="480.0" maxWidth="200.0" minHeight="480.0" minWidth="200.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="480.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="idText" alignment="CENTER" prefHeight="80.0" prefWidth="200.0" style="-fx-font-size: 20;" text="ID" />
                        <Label fx:id="pwText" alignment="CENTER" prefHeight="80.0" prefWidth="200.0" style="-fx-font-size: 20;" text="PW" />
                        <Label fx:id="nameText" alignment="CENTER" prefHeight="80.0" prefWidth="200.0" style="-fx-font-size: 20;" text="이름" />
                        <Label fx:id="phoneNoText" alignment="CENTER" prefHeight="80.0" prefWidth="200.0" style="-fx-font-size: 20;" text="전화번호" />
                        <Label fx:id="birthText" alignment="CENTER" prefHeight="80.0" prefWidth="200.0" style="-fx-font-size: 20;" text="생년월일" />
                        <Label fx:id="nickNameText" alignment="CENTER" prefHeight="80.0" prefWidth="200.0" style="-fx-font-size: 20;" text="닉네임" />
                     </children>
                  </VBox>
                  <VBox fx:id="inputBox" alignment="TOP_CENTER" prefHeight="480.0" prefWidth="300.0">
                     <children>
                        <BorderPane fx:id="idBP" prefHeight="80.0" prefWidth="200.0">
                           <center>
                              <TextField fx:id="idField" minHeight="40.0" prefHeight="40.0" prefWidth="300.0" promptText="Input your ID" BorderPane.alignment="CENTER">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </TextField>
                           </center>
                        </BorderPane>
                        <BorderPane fx:id="pwBP" prefHeight="80.0" prefWidth="200.0">
                           <center>
                              <TextField fx:id="pwField" minHeight="40.0" prefHeight="40.0" prefWidth="300.0" promptText="Input your password" BorderPane.alignment="CENTER">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </TextField>
                           </center>
                        </BorderPane>
                        <BorderPane fx:id="nameBP" prefHeight="80.0" prefWidth="200.0">
                           <center>
                              <TextField fx:id="nameField" minHeight="40.0" prefHeight="40.0" prefWidth="300.0" promptText="Input your name" BorderPane.alignment="CENTER">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </TextField>
                           </center>
                        </BorderPane>
                        <BorderPane fx:id="phoneNoBP" prefHeight="80.0" prefWidth="200.0">
                           <center>
                              <TextField fx:id="phoneNoField" minHeight="40.0" prefHeight="40.0" prefWidth="300.0" promptText="Input your phone number" BorderPane.alignment="CENTER">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </TextField>
                           </center>
                        </BorderPane>
                        <BorderPane fx:id="birthBP" prefHeight="80.0" prefWidth="200.0">
                           <center>
                              <DatePicker fx:id="birthField" prefHeight="40.0" prefWidth="300.0" BorderPane.alignment="CENTER" />
                           </center>
                        </BorderPane>
                        <BorderPane fx:id="nickNameBP" prefHeight="80.0" prefWidth="200.0">
                           <center>
                              <TextField fx:id="nickNameField" minHeight="40.0" prefHeight="40.0" prefWidth="300.0" promptText="Input your nickname" BorderPane.alignment="CENTER">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </TextField>
                           </center>
                        </BorderPane>
                     </children>
                  </VBox>
                  <VBox fx:id="checkBox" prefHeight="480.0" prefWidth="150.0">
                     <children>
                        <BorderPane fx:id="idCheckBP" prefHeight="80.0" prefWidth="200.0">
                           <center>
                              <Button fx:id="idCheckBtn" mnemonicParsing="false" onAction="#clickAction" text="중복확인" BorderPane.alignment="CENTER" />
                           </center>
                        </BorderPane>
                        <BorderPane prefHeight="80.0" prefWidth="200.0" />
                        <BorderPane prefHeight="80.0" prefWidth="200.0" />
                        <BorderPane prefHeight="80.0" prefWidth="200.0" />
                        <BorderPane prefHeight="80.0" prefWidth="200.0" />
                        <BorderPane fx:id="nickNameCheckBP" prefHeight="80.0" prefWidth="200.0">
                           <center>
                              <Button fx:id="nickNameCheckBtn" mnemonicParsing="false" onAction="#clickAction" text="중복확인" BorderPane.alignment="CENTER" />
                           </center>
                        </BorderPane>
                     </children>
                  </VBox>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <bottom>
                        <Button fx:id="signUpBtn" mnemonicParsing="false" onAction="#clickAction" text="가입하기" BorderPane.alignment="CENTER">
                           <BorderPane.margin>
                              <Insets bottom="20.0" />
                           </BorderPane.margin>
                        </Button>
                     </bottom>
                  </BorderPane>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
